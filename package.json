{
  "name": "pk-dms",
  "version": "1.0.0",
  "description": "A full stack document management system, complete with roles and privileges",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js --exec babel-node",
    "mig:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "mig:dev": "cross-env NODE_ENV=development sequelize db:migrate",
    "mig:test:undo": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "mig:dev:undo": "cross-env NODE_ENV=development sequelize db:migrate:undo:all",
    "test": "cross-env NODE_ENV=test istanbul cover _mocha -- -R spec test/**/**/*.spec.js --compilers js:babel-core/register",
    "seed:test": "cross-env NODE_ENV=test sequelize db:seed:all",
    "seed:dev": "cross-env NODE_ENV=development sequelize db:seed:all",
    "seed:create:test": "cross-env NODE_ENV=test sequelize seed:create",
    "seed:create:dev": "cross-env NODE_ENV=development sequelize seed:create",
    "seed:dev:undo": "cross-env NODE_ENV=development sequelize db:seed:undo:all",
    "seed:test:undo": "cross-env NODE_ENV=test sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-opikuda/documentManagementSystem.git"
  },
  "keywords": [
    "document",
    "management",
    "system"
  ],
  "author": "Pikuda.O.P",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-opikuda/documentManagementSystem/issues"
  },
  "homepage": "https://github.com/andela-opikuda/documentManagementSystem#readme",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "fm-log": "^4.4.3",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.3.0",
    "morgan": "^1.8.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "sequelize": "^3.30.3",
    "supertest": "^3.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "webpack-dev-server": "^1.4.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
