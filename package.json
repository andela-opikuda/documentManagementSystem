{
  "name": "pk-dms",
  "version": "1.0.0",
  "description": "A full stack document management system, complete with roles and privileges",
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack && nodemon app.js --exec babel-node",
    "mig:test": "NODE_ENV=test sequelize db:migrate",
    "mig:dev": "NODE_ENV=development sequelize db:migrate",
    "mig:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "mig:dev:undo": "NODE_ENV=development sequelize db:migrate:undo:all",
    "test": "NODE_ENV=test sequelize db:migrate && istanbul cover _mocha -- -R spec test/**/**/*.spec.js --compilers js:babel-core/register",
    "seed:test": "NODE_ENV=test sequelize db:seed:all",
    "seed:dev": "NODE_ENV=development sequelize db:seed:all",
    "seed:create:test": "NODE_ENV=test sequelize seed:create",
    "seed:create:dev": "NODE_ENV=development sequelize seed:create",
    "seed:dev:undo": "NODE_ENV=development sequelize db:seed:undo:all",
    "seed:test:undo": "NODE_ENV=test sequelize db:seed:undo:all",
    "e2e-setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "test:e2e": "npm run e2e:mig && node_modules/.bin/nightwatch --config nightwatch.conf.js",
    "e2e:mig": "npm run mig:dev:undo && npm run mig:dev && npm run seed:dev",
    "mocha": "mocha --compilers js:babel-core/register",
    "test:client": "mocha ./test/client/**/*.spec.js --compilers js:babel-register --require ./test/client/helpers.js --require ./test/client/dom.js --recursive",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover _mocha -- -R spec test/**/**/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "postinstall": "./node_modules/.bin/webpack && sequelize db:migrate && sequelize db:seed:undo:all && sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-opikuda/documentManagementSystem.git"
  },
  "keywords": [
    "document",
    "management",
    "system"
  ],
  "author": "Pikuda.O.P",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-opikuda/documentManagementSystem/issues"
  },
  "homepage": "https://github.com/andela-opikuda/documentManagementSystem#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "env2": "^2.1.1",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "file-loader": "^0.11.1",
    "fm-log": "^4.4.3",
    "istanbul": "^0.4.5",
    "izitoast": "^1.1.1",
    "jsdom": "^8.5.0",
    "jsonwebtoken": "^7.3.0",
    "jwt-decode": "^2.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.2",
    "nyc": "^10.3.2",
    "path": "^0.12.7",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-materialize": "^0.18.1",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.0.0",
    "react-s-alert": "^1.3.0",
    "react-test-renderer": "^15.5.4",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^4.1.1",
    "selenium-download": "^2.0.10",
    "sequelize": "^3.30.3",
    "sequelize-cli": "^2.7.0",
    "sinon": "^2.2.0",
    "style-loader": "^0.16.1",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.9.2",
    "sweetalert": "^1.1.3",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "nightwatch": "^0.9.15",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "selenium-standalone": "^6.4.1"
  }
}
